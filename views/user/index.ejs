<% include ../partials/header %>
  <div class='container'>
    <h6>Dashboard</h6>
    <h3>Welcome back, <%= user.firstName%> <%= user.lastName %>! </h3>
    <a href='/auth/logout' class='btn red darken-4'>Log Out</a>
    <h1>Your Favorites</h1>
    <ul class='favlist'>
    <% if (faves !== undefined) { %>
     <% faves.forEach((fave) => { %>
       <li><a href=<%= `/movies/${fave.id}`%> ><%= fave.title %></a></li>
      <% }); %>
    <% } %>
    </ul>
  </div>

<% include ../partials/footer %>